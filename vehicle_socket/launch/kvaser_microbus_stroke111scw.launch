<launch>
	<arg name="kvaser_channel" default="0"/>
	<arg name="use_velocity_topic" default="1"/>

	<node pkg="vehicle_socket" type="kvaser_microbus_can_receiver111scw" name="kvaser_microbus_can_receiver111scw" output="screen">
		<param name="kvaser_channel" value="$(arg kvaser_channel)"/>
	</node>

	<node pkg="vehicle_socket" type="kvaser_microbus_can_sender111scw" name="kvaser_microbus_can_sender111scw" output="log">
		<param name="kvaser_channel" value="$(arg kvaser_channel)"/>
		<param name="use_velocity_topic" value="$(arg use_velocity_topic)"/>
	</node>
</launch>
